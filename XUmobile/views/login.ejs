<%- include header %>

<section id="main">
        <div id="loginwrap">
        <section id="login">
            <header id="logo">
                <img src="images/login/logo.png" alt=""/>
            </header>
            <section id="formwarp">
                <form action="#">
                    <input type="text" id="username" class='input_unfocus' name="username" placeholder="请输入用户名或邮箱" />
                    <input type="password" id="password"class='input_unfocus' name="password" placeholder="密码" />
                    <!--注册按钮默认绿色，在填写表单数据通过本地验证后登陆按钮变绿-->
                    <input class='btn unava' type="button" id="confirmlogin" name="confirmlogin" value="登陆" />
                    <input class='btn ava' id="register" name="register" value="创建用户" type="button"/>
                </form>
            </section>
            <footer id="errortip">您输入的密码有误。</footer>
        </section>
    </div>
</section>

</html>